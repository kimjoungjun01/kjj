package step27.ex12;

import org.springframework.stereotype.Component;

@Component("dellta")
public class DelltaEngine extends Engine{
  boolean hybrid;

  
  @Override
  public String toString() {
    return "DelltaEngine [hybrid=" + hybrid + ", valve=" + valve + ", cylinder=" + cylinder + "]";
  }

  public boolean isHybrid() {
    return hybrid;
  }

  public void setHybrid(boolean hybrid) {
    this.hybrid = hybrid;
  }
  
}
